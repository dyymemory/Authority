<fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
    <legend>职务权限</legend>
</fieldset>
<form class="layui-form">
    <div class="layui-form-item">
        <div class="layui-col-md9">
            <label class="layui-form-label">城市</label>
            <div class="layui-input-inline">
                <select id="City" lay-search="" lay-filter="City"></select>
            </div>
            <label class="layui-form-label">职务</label>
            <div class="layui-input-inline">
                <select id="Position" lay-filter="Position" lay-search=""></select>
            </div>
        </div>
        <div class="layui-col-md3">
            <div class="layui-input-inline">
                <a href="javascript:;" id="Search" lay-filter="Search" class="layui-btn">查询</a>
                <button type="reset" class="layui-btn layui-btn-primary">重置</button>
            </div>
        </div>
    </div>
    <div class="layui-form-item">
        <table class="layui-hide" id="GridTalbePosition" lay-filter="PositionTableDate"></table>
        <div id="page"></div>
    </div>
</form>


<script type="text/html" id="Operationfield">
    <a class="layui-btn layui-btn-xs" lay-event="view">查看</a>
</script>

<script src="../../Content/jscustomer/position/PositionIndex.js" charset="utf-8"></script>
<script src="../../Content/jscustomer/CacheHelper.js"></script>
